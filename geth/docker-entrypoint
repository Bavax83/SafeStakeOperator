#!/bin/sh
(geth \
--ropsten \
--http \
--http.addr="0.0.0.0"  
--datadir /data/geth \
--metrics \
--metrics.expensive \
--pprof \
--http.api="engine,eth,web3,net,debug" \
--http.corsdomain "*" \
--authrpc.jwtsecret=/data/geth/jwtsecret \
--override.terminaltotaldifficulty 50000000000000000 > geth_output 2>&1 )
